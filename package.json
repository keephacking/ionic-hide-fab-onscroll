{
    "name": "ionic-hide-fab-onscroll",
    "version": "1.0.0",
    "description": "A simple plugin to hide the ionic fab buttons while scrolling",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "tsc": "tsc -p tsconfig.json",
        "umd": "rollup -c rollup.config.umd.js",
        "rollup": "rollup -c rollup.config.js",
        "esm": "rollup -c rollup.config.esm.js",
        "copy": "node tools/copy-package.js && node tools/clean-package.js",
        "bundle": "rimraf dist && npm run umd && npm run esm && tsc",
        "postbundle": "node tools/delete-unwanted.js",
        "build1": "npm run bundle && npm run copy",
        "build": "rm -rf aot dist && npm run ngc",
        "ngc": "ngc"
    },
    "keywords": ["angular5", "ionic3", "ionic2", "fab"],
    "author": "Sachin M S",
    "license": "MIT",
    "peerDependencies": {
        "ionic-angular": "^3.9.2"
    },
    "devDependencies": {
        "del": "^3.0.0",
        "node-sass": "^4.7.2",
        "rollup-plugin-angular": "^0.5.3",
        "rollup-plugin-commonjs": "^8.2.6",
        "rollup-plugin-node-resolve": "^3.0.0",
        "rollup-plugin-typescript": "^0.8.1",
        "@angular/core": "^5.1.2",
        "rxjs": "^5.5.6",
        "zone.js": "^0.8.18",
        "ionic-angular": "^3.9.2",
        "@angular/common": "^5.1.2",
        "@angular/compiler": "^5.1.2",
        "@angular/compiler-cli": "^5.1.2",
        "typescript": "2.6.2"
    }
}